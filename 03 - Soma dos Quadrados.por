programa {
  funcao inicio() {
    real num1, num2, resultado = 0

    escreva ("Digite o primeiro número\n")
    leia (num1)
    escreva ("Digite o segundo número:\n")
    leia (num2)

    resultado = (num1  * num1) + (num2 + num2)
    limpa()
    escreva ("A soma dos quadrados é ", resultado)    
  }
}
